{
  "version": 2,
  "functions": {
    "backend/index.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|html))",
      "dest": "/$1"
    },
    {
      "src": "/paginas/(.*)",
      "dest": "/paginas/$1"
    },
    {
      "src": "/conexion/(.*)",
      "dest": "/conexion/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
