{
  "version": 2,
  "builds": [
    {
      "src": "conexion/vercel_handler.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "conexion/vercel_handler.py"
    },
    {
      "src": "/paginas/(.*)",
      "dest": "/paginas/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
