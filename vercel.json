{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.py"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|html))",
      "dest": "/$1"
    },
    {
      "src": "/paginas/(.*)",
      "dest": "/paginas/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
